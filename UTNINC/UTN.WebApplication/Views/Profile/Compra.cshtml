@using UTN.Inc.Entities
@model List<Producto>

@{
    ViewData["Title"] = "Compra";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<ul>
    <li>@Html.ActionLink("Perfil", "Profile", "Login")</li>
</ul>
<h1>Compra</h1>

<ul>
    @foreach (var product in Model)
    {
        <li>Producto: @product.Nombre</li>
    }
    
</ul>
